#! /usr/bin/python

import subprocess

# In the actual contest you'd want to run netcat or just
# open your own TCP connection to port 23 and run commands.
d = subprocess.Popen(['./dillo'],
                     stdout=subprocess.PIPE,
                     stdin=subprocess.PIPE)
o = d.stdout
i = d.stdin

c = o.read(1)
v = chr(ord(c) ^ 0x20)
i.write(v)

o.readline()
o.readline()
o.readline()
o.readline()
o.readline()
d.poll()
